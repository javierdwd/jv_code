  "bugs": {
    "url": "https://github.com/javierdwd/jv_code/issues"
  },
  "homepage": "https://github.com/javierdwd/jv_code#readme",
  "dependencies": {
    "jv_code_github_activity": "github:javierdwd/jv_code_github_activity#master",
    "parcel-bundler": "1.12.3",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "autoprefixer": "9.5.1",
    "postcss-nested": "4.1.2",
    "posthtml-modules": "0.4.2"
